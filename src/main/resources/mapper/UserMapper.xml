        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.allspark.allsparkofficialwebsite.mapper.UserMapper">

    <resultMap id="BaseResultMap" type="com.allspark.allsparkofficialwebsite.model.User">
